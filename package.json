{
  "name": "whinepad",
  "version": "2.0.0",
  "scripts": {
    "watch": "watch \"sh ./scripts/build.sh\" js/source css/",
    "build": "sh scripts/build.sh",
    "test": "jest"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "babel",
      "react"
    ],
    "extends": "eslint:recommended",
    "env": {
      "browser": true
    },
    "rules": {
      "react/jsx-uses-react": 1,
      "no-unused-vars": "off",
      "no-extra-semi": "off"
    }
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "classnames": "^2.2.6",
    "eslint": "^7.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^26.0.1",
    "react-test-renderer": "^16.13.1"
  }
}
